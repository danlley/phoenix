<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"   
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
       xmlns:context="http://www.springframework.org/schema/context"   
       xmlns:mvc="http://www.springframework.org/schema/mvc"  
       xsi:schemaLocation="http://www.springframework.org/schema/mvc   
       http://www.springframework.org/schema/mvc/spring-mvc-4.1.xsd  
        http://www.springframework.org/schema/beans   
                http://www.springframework.org/schema/beans/spring-beans-4.1.xsd  
        http://www.springframework.org/schema/context   
                http://www.springframework.org/schema/context/spring-context-4.1.xsd"
	default-autowire="byName">
	<bean id="pxCommonManageComponent" class="com.myteay.phoenix.core.service.manage.component.impl.PxCommonManageComponentImpl"></bean>
	
	<bean id="pxDataDictionaryComponent" class="com.myteay.phoenix.core.service.component.impl.PxDataDictionaryComponentImpl"></bean>
	
	<bean id="pxMobileGoodsCacheComponent" class="com.myteay.phoenix.core.service.component.impl.PxMobileGoodsCacheComponentImpl"></bean>
	
	<bean id="pxMobileGoodsComponnet" class="com.myteay.phoenix.core.service.component.impl.PxMobileGoodsComponnetImpl"></bean>
	
	<bean id="pxGoodsStrategy" class="com.myteay.phoenix.core.service.strategy.impl.PxGoodsStrategyImpl"></bean>
	
	<bean id="pxShopComponent" class="com.myteay.phoenix.core.service.manage.component.impl.PxShopComponentImpl"></bean>
	
	<bean id="pxGoodsComponent" class="com.myteay.phoenix.core.service.manage.component.impl.PxGoodsComponentImpl"></bean>
	
	<bean id="pxGoodsPackagesDetailComponent" class="com.myteay.phoenix.core.service.manage.component.impl.PxGoodsPackagesDetailComponentImpl"></bean>
	
	<bean id="pxSubPackagesComponent" class="com.myteay.phoenix.core.service.manage.component.impl.PxSubPackagesComponentImpl"></bean>
	
	<bean id="pxGoodsPackagesImageComponent" class="com.myteay.phoenix.core.service.manage.component.impl.PxGoodsPackagesImageComponentImpl"></bean>
	
	<bean id="pxGoodsPackagesNoticeComponent" class="com.myteay.phoenix.core.service.manage.component.impl.PxGoodsPackagesNoticeComponentImpl"></bean>
	
	<bean id="pxGoodsPackagesSubNoticeComponent" class="com.myteay.phoenix.core.service.manage.component.impl.PxGoodsPackagesSubNoticeComponentImpl"></bean>
	
	<bean id="pxGoodsStatusComponent" class="com.myteay.phoenix.core.service.manage.component.impl.PxGoodsStatusComponentImpl"></bean>
	
	<bean id="pxCommonMngTemplate" class="com.myteay.phoenix.core.service.manage.template.PxCommonMngTemplateImpl"></bean>
	
	<bean id="campShopBaseComponent" class="com.myteay.phoenix.core.service.camp.component.impl.CampShopBaseComponentImpl"></bean>
	
	<bean id="campShopBaseStatusComponent" class="com.myteay.phoenix.core.service.camp.component.impl.CampShopBaseStatusComponentImpl"></bean>
	
	<bean id="campShopPrizeComponent" class="com.myteay.phoenix.core.service.camp.component.impl.CampShopPrizeComponentImpl"></bean>
	
	<bean id="campShopPrizeRefGoodsComponent" class="com.myteay.phoenix.core.service.camp.component.impl.CampShopPrizeRefGoodsComponentImpl"></bean>
	
	<!-- 商品状态变更为上线监听器 -->
	<bean id="pxGoodsStatusChangedEventListener" class="com.myteay.phoenix.core.service.manage.listener.PxGoodsStatusChangedEventListener">
		<property name="descriptor">
			<bean class="com.myteay.common.async.event.ListenerDescriptor">
				<property name="topic" value="PX_GOODS_STATUS_CHANGED"></property>
				<property name="async" value="true"></property>
				<property name="taskExecutor" ref="myteayHigherTaskExecutor"></property>
			</bean>
		</property>
	</bean>	
</beans>
